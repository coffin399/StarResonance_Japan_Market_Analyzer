{
  "productName": "StarResonance Market Analyzer",
  "version": "0.1.0",
  "identifier": "com.starresonance.marketanalyzer",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../build"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": ["resources/*"],
    "externalBin": [],
    "copyright": "",
    "shortDescription": "Blue Protocol: Star Resonance Market Analyzer",
    "longDescription": "A tool to capture and analyze market data from Blue Protocol: Star Resonance using WinDivert packet capture.",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "wix": {
        "language": "ja-JP"
      },
      "webviewInstallMode": {
        "type": "downloadBootstrapper"
      },
      "allowDowngrades": true
    }
  },
  "app": {
    "windows": [
      {
        "title": "StarResonance Market Analyzer",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": false,
        "center": true,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  }
}
